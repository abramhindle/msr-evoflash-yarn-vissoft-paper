@Unpublished{church04,
  author =   {Derek Church},
  title =    {A survey of techniques for the recovery and observation
                 of Software Evolution},
  institution =  {University of Victoria},
  year =     {2004},
 note = {Unpublished},
  abstract = {This overview presents recent research on the extraction
of information produced during the creation of a software product,
data-mining of that information and the visualization of the
results. The purpose of this research is to capture the process of
evolution in the software development lifecycle. By observing trends
and attempting to establish patterns in the evolutionary process it is
hoped that new insight can be gained towards improving software
engineering practices as well as validation of current techniques.},

